<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<ui-command xmlns="http://www.efaps.org/xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.efaps.org/xsd http://www.efaps.org/xsd/eFaps_1.0.xsd">
    <uuid>f0450ba8-837e-48ca-938c-9327d32fcd66</uuid>
    <file-application>eFapsApp-Payroll</file-application>
    <definition>
        <version-expression>(version==latest)</version-expression>
        <name>Payroll_CashOutReportMyDesk_Menu_DownloadPDF</name>
        <icon>Document_PDF</icon>
        <access>
            <role>Payroll_Admin</role>
        </access>
        <target>
            <execute method="exportReport" program="org.efaps.esjp.payroll.reports.CashOutReport">
                <property name="Mime">pdf</property>
                <property name="PageType">A4</property>
                <property name="PageOrientation">LANDSCAPE</property>
                <property name="FilterKey">org.efaps.esjp.payroll.reports.CashOutReport.FilterKey</property>
            </execute>
        </target>
        <property name="Target">hidden</property>
        <property name="TargetShowFile">true</property>
        <property name="Label">report.DownloadPDF</property>
    </definition>
</ui-command>
