<?xml version="1.0" encoding="UTF-8" standalone="no"?>


<ui-form xmlns="http://www.efaps.org/xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.efaps.org/xsd http://www.efaps.org/xsd/eFaps_1.0.xsd">
    <uuid>82f2697b-4bea-41fe-a729-8c8d8fdbb68a</uuid>
    <file-application>eFapsApp-Payroll</file-application>
    <definition>
        <version-expression>(version==latest)</version-expression>
        <name>Payroll_LaborTimeReportForm</name>
        <field name="filter">
            <property name="HideLabel">true</property>
            <property name="UIType">SNIPPLET</property>
            <trigger event="UI_FIELD_VALUE" index="1" method="setDefaultFilter"
                name="Payroll_LaborTimeReportForm.filter.UI_FIELD_VALUE.1"
                program="org.efaps.esjp.payroll.reports.LaborTimeReport">
                <property name="FilterKey">org.efaps.esjp.payroll.reports.LaborTimeReport.FilterKey</property>
                <property name="ReportField">report4Account</property>
                <property name="Field01">dateFrom</property>
                <property name="Field02">dateTo</property>
                <property name="Field03">employee</property>
                <property name="Field04">settlement</property>
                <property name="Field05">monthly</property>
                <property name="FilterType01">DateTime</property>
                <property name="FilterType02">DateTime</property>
                <property name="FilterType03">Instance</property>
                <property name="FilterType04">Boolean</property>
                <property name="FilterType05">Boolean</property>
                <property name="FilterDefault01">MONTH:-1</property>
                <property name="FilterDefault02">TODAY</property>
                <property name="FilterDefault03">-</property>
                <property name="FilterDefault04">false</property>
                <property name="FilterDefault05">false</property>
            </trigger>
            <trigger event="UI_FIELD_VALUE" index="2" method="getFilterFieldValue"
                name="Payroll_LaborTimeReportForm.filter.UI_FIELD_VALUE.2"
                program="org.efaps.esjp.payroll.reports.LaborTimeReport">
                <property name="FilterKey">org.efaps.esjp.payroll.reports.LaborTimeReport.FilterKey</property>
                <property name="Field01">dateFrom</property>
                <property name="Field02">dateTo</property>
                <property name="Field03">employee</property>
                <property name="Field04">settlement</property>
                <property name="Field05">monthly</property>
                <property name="DBProperty01">org.efaps.esjp.payroll.reports.LaborTimeReport.dateFrom</property>
                <property name="DBProperty02">org.efaps.esjp.payroll.reports.LaborTimeReport.dateTo</property>
                <property name="DBProperty03">org.efaps.esjp.payroll.reports.LaborTimeReport.employee</property>
                <property name="DBProperty04">org.efaps.esjp.payroll.reports.LaborTimeReport.settlement</property>
                <property name="DBProperty05">org.efaps.esjp.payroll.reports.LaborTimeReport.monthly</property>
                <property name="HumanResource_Employee_MsgPhrase">HumanResource_EmployeeWithNumberMsgPhrase</property>
            </trigger>
        </field>
        <field name="report">
            <property name="HideLabel">true</property>
            <property name="UIType">SNIPPLET</property>
            <trigger event="UI_FIELD_VALUE" method="generateReport"
                name="Payroll_LaborTimeReportForm.report.UI_FIELD_VALUE"
                program="org.efaps.esjp.payroll.reports.LaborTimeReport">
                <property name="FilterKey">org.efaps.esjp.payroll.reports.LaborTimeReport.FilterKey</property>
                <property name="Type">Payroll_Payslip</property>
                <property name="StatusGroup">Payroll_PayslipStatus</property>
                <property name="Status">!Canceled</property>
            </trigger>
        </field>
    </definition>
</ui-form>
