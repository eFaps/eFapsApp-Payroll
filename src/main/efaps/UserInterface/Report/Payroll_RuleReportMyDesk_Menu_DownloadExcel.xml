<?xml version="1.0" encoding="UTF-8" standalone="no"?>


<ui-command xmlns="http://www.efaps.org/xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.efaps.org/xsd http://www.efaps.org/xsd/eFaps_1.0.xsd">
    <uuid>5f8ac5f8-a5f5-4e11-9092-2b6232314b67</uuid>
    <file-application>eFapsApp-Payroll</file-application>
    <definition>
        <version-expression>(version==latest)</version-expression>
        <name>Payroll_RuleReportMyDesk_Menu_DownloadExcel</name>
        <icon>Document_Excel</icon>
        <access>
            <role>Sales_Admin</role>
        </access>
        <target>
            <execute method="exportReport" program="org.efaps.esjp.payroll.reports.RuleReport">
                <property name="Mime">xls</property>
                <property name="FilterKey">org.efaps.esjp.payroll.reports.RuleReport.FilterKey</property>
                <property name="Type">Payroll_Payslip</property>
                <property name="StatusGroup">Payroll_PayslipStatus</property>
                <property name="Status">!Canceled</property>
            </execute>
        </target>
        <property name="Target">hidden</property>
        <property name="TargetShowFile">true</property>
        <property name="Label">report.DownloadExcel</property>
    </definition>
</ui-command>
