<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui-command xmlns="http://www.efaps.org/xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.efaps.org/xsd http://www.efaps.org/xsd/eFaps_1.0.xsd">

    <uuid>2465301b-145b-40a4-a801-425f8d7a727c</uuid>
    <file-application>eFapsApp-Payroll</file-application>
    

    <definition>
        <version-expression>(version==latest)</version-expression>
        <name>Payroll_PayslipTree_Payment_Menu_Action_Create</name>

        <access>
            <role>Sales_Admin</role>
        </access>

        

        <target>
            <form>Sales_PaymentOutForm</form>
            <execute method="create" program="org.efaps.esjp.sales.Payment">
                <property name="JasperReport">Sales_PaymentDocumentReport</property>
                <property name="Mime">pdf</property>
            </execute>
        </target>

        

        <property name="Target">modal</property>
        <property name="TargetCreateType">Sales_Payment</property>
        <property name="TargetConnectAttribute">Sales_Payment/CreateDocument</property>
        <property name="TargetMode">create</property>
        <property name="WindowHeight">460</property>
        <property name="WindowWidth">720</property>
    </definition>

</ui-command>
